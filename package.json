{
  "name": "MME Projects",
  "version": "0.0.1",
  "description": "A template for your MME Projects",
  "type": "module",
  "main": "index.js",
  "watch": {
    "runDevelopementServer": {
      "patterns": [
        "app/src"
      ],
      "extensions": "js",
      "inherit": true,
      "clearBuffer": true
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm-watch",
    "runDevelopementServer": "node start_dev_server.js",
    "build": "node build_for_deployment.js",
    "check": "eslint app/src/",
    "prebuild": "npm install && npm run check"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "dotenv": "^16.0.0",
    "eslint": "^8.8.0",
    "express": "^4.17.3",
    "fs-extra": "^10.0.0",
    "npm-watch": "^0.11.0",
    "open": "^8.4.0"
  },
  "dependencies": {
    "appwrite": "^7.0.0",
    "bootstrap": "^5.1.3",
    "browserify-shim": "^3.8.14",
    "edit-json-file": "^1.7.0",
    "fs": "^0.0.1-security",
    "http": "^0.0.1-security",
    "node-appwrite": "^5.0.0",
    "node.js": "^0.0.1-security",
    "socket.io": "^4.4.1",
    "uuid": "^8.3.2"
  }
}
